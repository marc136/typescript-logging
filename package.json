{
  "name": "loglevel-ts",
  "description": "Tiny logger with slim api and ability of limitless extension",
  "keywords": [
    "log",
    "logger",
    "logging",
    "loglevel",
    "browser",
    "node"
  ],
  "repository": "github:marc136/loglevel-ts",
  "author": "Marc Walter <walter.marc@outlook.com>",
  "license": "MIT",
  "version": "0.1.0",
  "source": "src/log.ts",
  "main": "dist/log.js",
  "module": "dist/log.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch --project tsconfig.json",
    "clean": "node scripts/clean",
    "build:module": "tsc --project tsconfig.json",
    "build:browser": "node scripts/convert-for-browser",
    "prepublish": "npm run clean && npm run build:module && npm run build:browser"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
